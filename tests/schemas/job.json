{
   "$schema":"http://json-schema.org/draft-04/schema#",
   "title":"Job Schema",
   "type":"object",
   "properties":{
      "uid":{
         "type":"integer"
      },
      "creation_time":{
         "type":"integer"
      },
      "content":{
         "type":"string"
      },
      "cost_function":{
         "description":"List of cost function attributes",
         "type":"object",
         "properties":{
            "type":{
               "type":"string"
            },
            "reward":{
               "type":"integer"
            },
            "penalty":{
               "type":"integer"
            },
            "valid_time":{
               "type":"integer"
            }
         },
         "required":[
            "type",
            "reward",
            "penalty",
            "valid_time"
         ]
      },
      "pick_up":{
         "type":"object"
      },
      "destination":{
         "type":"object"
      }
   },
   "required":[
      "uid",
      "creation_time",
      "content",
      "cost_function",
      "pick_up",
      "destination"
   ]
}
